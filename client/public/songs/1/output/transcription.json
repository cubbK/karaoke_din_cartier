    {
        "start": 16.0,
        "end": 18.0,
        "text": " Nubi"
    },
    {
        "start": 18.0,
        "end": 22.0,
        "text": " Am în minte acum vremurile de început"
    },
    {
        "start": 22.0,
        "end": 24.0,
        "text": " Cand ma gândeam cum sa fac bani"
    },
    {
        "start": 24.0,
        "end": 26.0,
        "text": " Ce e de făcut cu tovarăși filmei"
    },
    {
        "start": 26.0,
        "end": 28.0,
        "text": " Săteam si ne-ntrebam oare muzica asta"
    },
    {
        "start": 28.0,
        "end": 29.72,
        "text": " Asta o să ne aducă niște bani"
    },
    {
        "start": 29.72,
        "end": 32.02,
        "text": " Am încercat multe feluri de-a o face"
    },
    {
        "start": 32.02,
        "end": 34.8,
        "text": " Și până la urmă am pot în râd să facem ce ne place"
    },
    {
        "start": 34.8,
        "end": 37.36,
        "text": " Am fi putut foarte ușor să facem un stilu american"
    },
    {
        "start": 37.36,
        "end": 39.78,
        "text": " Dar la noi acasă e stilu sălășean"
    },
    {
        "start": 39.78,
        "end": 42.84,
        "text": " Manager, impresor, producător când ai suceo vorbesc frumos"
    },
    {
        "start": 42.84,
        "end": 44.760000000000005,
        "text": " Despre-ncredere zâmbesc periculos"
    },
    {
        "start": 44.760000000000005,
        "end": 47.5,
        "text": " Ceva nou pentru unul de pe stradă"
    },
    {
        "start": 47.5,
        "end": 49.900000000000006,
        "text": " Pentru care să vândă m-ar fărea singura șansă"
    },
    {
        "start": 49.900000000000006,
        "end": 52.72,
        "text": " Dacă nu știi cum să treabă alții profite de pe urma ta"
    },
    {
        "start": 52.72,
        "end": 54.92,
        "text": " Fiindcă ești bagabon, nu ești cineva"
    },
    {
        "start": 54.92,
        "end": 58.08,
        "text": " Am adus strada în mijlocul vedețelor de prima mai"
    },
    {
        "start": 58.08,
        "end": 60.54,
        "text": " Am fost periat, nu știau, oare e adevărat?"
    },
    {
        "start": 60.620000000000005,
        "end": 62.24,
        "text": " Ce dracu e cu bagabonții ăștia?"
    },
    {
        "start": 62.36,
        "end": 65.26,
        "text": " Îmbrăcați ciudat, pămâși în carăuri, pantaloni, ca chi de fapt"
    },
    {
        "start": 65.26,
        "end": 67.1,
        "text": " În loc de șlagăre, obosite"
    },
    {
        "start": 67.1,
        "end": 69.84,
        "text": " Le-am adus hit-uri, cum bagabon tu simte?"
    },
    {
        "start": 69.92,
        "end": 72.52000000000001,
        "text": " Deodată s-a întâmplat ceva, nu știu, stresul e de vină"
    },
    {
        "start": 72.52000000000001,
        "end": 74.8,
        "text": " Și ultimul refugiu l-am găsit în heroină"
    },
    {
        "start": 74.8,
        "end": 77.38,
        "text": " Îmi pare rău că s-a întâmplat așa, asta-i viața"
    },
    {
        "start": 77.38,
        "end": 79.86,
        "text": " Fratele meu cu ea, mi-a redat speranța"
    },
    {
        "start": 79.86,
        "end": 82.76,
        "text": " Am înțeles că vii sunt la droguri, am uitat să mai visez"
    },
    {
        "start": 82.76,
        "end": 85.9,
        "text": " Știu că bagabonți acasă așteaptă să ransez un nou album"
    },
    {
        "start": 85.9,
        "end": 89.86,
        "text": " Nu ui dezamăgesc și ne-am întors pentru că îmi place să trăiesc"
    },
    {
        "start": 89.86,
        "end": 92.0,
        "text": " Poate nu am fost destul de tare"
    },
    {
        "start": 92.0,
        "end": 95.64,
        "text": " Nu mi-e rușine să o spun, am fost trăiat pe heroină frățioare"
    },
    {
        "start": 95.64,
        "end": 97.82,
        "text": " Știi că m-am lăsat, nu campania antidrog"
    },
    {
        "start": 97.82,
        "end": 100.02,
        "text": " M-a convins, dar simțeam că ar mai fi ceva de zis"
    },
    {
        "start": 100.02,
        "end": 102.52,
        "text": " Așa cum e, am făcut ce trebuia"
    },
    {
        "start": 102.52,
        "end": 104.88,
        "text": " N-am ținut, că-mi de regulă am rămas la fel"
    },
    {
        "start": 104.88,
        "end": 109.32,
        "text": " La fel pentru catier și pentru muzica de stradă care ne-a scos din el"
    },
    {
        "start": 109.32,
        "end": 112.5,
        "text": " Așa cum e, am făcut ce trebuia"
    },
    {
        "start": 112.5,
        "end": 114.86,
        "text": " N-am ținut, că-mi de regulă am rămas la fel"
    },
    {
        "start": 114.86,
        "end": 119.28,
        "text": " La fel pentru catier și pentru muzica de stradă care ne-a scos din el"
    },
    {
        "start": 119.28,
        "end": 122.39999999999999,
        "text": " Am început de jos, am avut speranță"
    },
    {
        "start": 122.39999999999999,
        "end": 124.96,
        "text": " Cu tupeu te borfos și am ieșit în față"
    },
    {
        "start": 124.96,
        "end": 127.96,
        "text": " Drumul care a urmat nu a fost, nu a fost cu bani"
    },
    {
        "start": 127.96,
        "end": 129.82,
        "text": " Și din cartier nu am scăpat"
    },
    {
        "start": 129.82,
        "end": 132.12,
        "text": " Ce am câștigat mai târziu s-a dus"
    },
    {
        "start": 132.12,
        "end": 135.23999999999998,
        "text": " Ce a trebuit să înșel, să fur, să stau în stres"
    },
    {
        "start": 135.23999999999998,
        "end": 137.62,
        "text": " Dar nu mă tem încă bag, grime pe beat"
    },
    {
        "start": 137.62,
        "end": 140.2,
        "text": " Că lumea tot mă întreabă de ce nu mă spatez nimic"
    },
    {
        "start": 140.2,
        "end": 143.4,
        "text": " Trebuie să zic ceva, trebuie să-mi vând caseta"
    },
    {
        "start": 143.4,
        "end": 145.16,
        "text": " Să fiu tot eu puia"
    },
    {
        "start": 145.16,
        "end": 147.62,
        "text": " Nu muzica împărât în goana după bani"
    },
    {
        "start": 147.62,
        "end": 150.35999999999999,
        "text": " Să mă fut pe respectul câștigat în cinci ani"
    },
    {
        "start": 150.35999999999999,
        "end": 152.95999999999998,
        "text": " Inspirația nu s-a dus subiecte, sunt destule"
    },
    {
        "start": 152.95999999999998,
        "end": 155.51999999999998,
        "text": " Stilul bagabunților are versuri dure"
    },
    {
        "start": 155.51999999999998,
        "end": 157.84,
        "text": " Că nu e doar că aia târfele îmi se dea din cur"
    },
    {
        "start": 157.84,
        "end": 160.02,
        "text": " Și puștigat să-i simtă bine că în jur"
    },
    {
        "start": 160.02,
        "end": 162.5,
        "text": " Așa cum e, am făcut ce trebuia"
    },
    {
        "start": 162.5,
        "end": 164.86,
        "text": " N-am ținut cum de regulă am rămas la fel"
    },
    {
        "start": 164.86,
        "end": 168.08,
        "text": " La fel pentru catieri și pentru muzica de stradă"
    },
    {
        "start": 168.08,
